.panel.panel-default{"id" => "Education_#{asset.id}"}
  .panel-heading
    .row
      .col-md-10.col-sm-10
        %strong= asset.institution_name
        %em= asset.focus
        = asset.completion.strftime('%b %Y')
      .col-md-2.col-sm-2.button-col
        = render "/assets/button_row", asset: asset, asset_type: asset_type
  .row
    .panel-body
      %ul
        - asset.descriptions.each do |description|
          %li= description.detail
        %em= asset.location

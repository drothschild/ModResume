.panel.panel-default{"id" => "Experience_#{asset.id}"}
  .panel-heading
    .row
      .col-md-10.col-sm-10
        %strong= asset.title
        %em= asset.company
        = asset.begin_date.strftime('%b %Y')
        to
        = asset.end_date.strftime('%b %Y')
      .col-md-2.col-sm-2
        = render "/assets/button_row", asset: asset, asset_type: asset_type

  = render 'descriptions/show', asset_descriptions: asset_descriptions, asset: asset, asset_type: asset_type

